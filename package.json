{
  "name": "@rghat/do-it-cli",
  "version": "1.0.2",
  "description": "Command-line interface for listing and running npm scripts.",
  "author": "Renee Ghattas <renee.ghattas@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/rghattas/doit-cli/blob/master/README.md",
  "engines": {
    "node": ">=8"
  },
  "bin": {
